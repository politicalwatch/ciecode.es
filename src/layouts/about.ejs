                <% if (data.team[teamindex].linkedin != "") { %>
                <li><a href="<%= data.team[teamindex].linkedin %>" class="color-linkedin full-round" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                <% } %>
                <% if (data.team[teamindex].github != "") { %>
                <li><a href="<%= data.team[teamindex].github %>" class="color-linkedin full-round" target="_blank"><i class="fa fa-github"></i></a></li>
                <% } %>
              </ul>
            </div>
